{
  "name": "ss-js-qa",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .js,.mjs,.cjs",
    "lint:fix": "eslint . --ext .js,.mjs,.cjs --fix",
    "dev:vanilla": "pnpm -F @ss-qa/vanilla dev",
    "build:vanilla": "pnpm -F @ss-qa/vanilla build",
    "test:vanilla": "pnpm -F @ss-qa/vanilla test",
    "coverage:vanilla": "pnpm -F @ss-qa/vanilla coverage",
    "dev:react": "pnpm -F @ss-qa/react dev",
    "build:react": "pnpm -F @ss-qa/react build",
    "dev:vue": "pnpm -F @ss-qa/vue dev",
    "build:vue": "pnpm -F @ss-qa/vue build",
    "build": "pnpm -r --filter @ss-qa/vanilla build",
    "test": "pnpm -r --filter @ss-qa/vanilla test"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.3.3"
  }
}
